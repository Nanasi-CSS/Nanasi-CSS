{
  "name": "nanasi-css",
  "version": "1.0.0",
  "description": "Nanasi.CSS is a responsive and pure CSS framework based on Flexbox. Made with ❤️ in Tanzania.",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "yarn run build-clean && yarn run build-css",
    "build-css": "postcss --use autoprefixer -o ./src/nanasi.css ./dist/nanasi.css",
    "build-clean": "rimraf dist"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "postcss-cli": "^6.1.3",
    "rimraf": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphaolomi/Nanasi-CSS.git"
  },
  "keywords": [],
